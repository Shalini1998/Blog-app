<h1>Hello, Rails!</h1>
<%= link_to 'My Blog', controller: 'dictionaries' %>

<div class="row">
  <%= search_form_for @q do |f| %>
    <div class="col-md-9">
      <div class="form-group">
        <%= f.search_field :title_cont , class: "form-control"%>
      </div>
    </div>
    <div class="col-md-3">
      <div class="form-group">
        <%= f.submit class: "btn btn-primary btn-block"%>
      </div>
    </div>
  <% end %>
</div>